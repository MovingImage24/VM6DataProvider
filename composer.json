{
  "name": "movingimage/data-provider-vm6",
  "description": "Dataprovider implementation that retrieves collection data from Video Manager 6",
  "type": "library",
  "license": "Proprietary",
  "authors": [
    {
      "name": "Robert Szeker",
      "email": "robert.szeker@movingimage.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://packagist.mi24.tv/"
    }
  ],
  "config": {
    "secure-http": false
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "movingimage/data-provider": "^1.0.0",
    "movingimage/vm6-api-client": "^0.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.5 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "MovingImage\\DataProvider\\": "lib/DataProvider"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MovingImage\\DataProvider\\Tests\\": "tests/DataProvider"
    }
  }
}